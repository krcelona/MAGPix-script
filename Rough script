assay = pd.read_csv("Assay_metadata.txt", sep = "\t")

dictList = {}
protein = []
with open('protein_dict.txt', 'r') as f:
    for line in f:
        elements = line.rstrip().split("\t")
        protein.append(elements)
        dictList[elements[0]] = elements[1] 


